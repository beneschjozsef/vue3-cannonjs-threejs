{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "preserve",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "strict",
  "vueIndentScriptAndStyle": true,
  "endOfLine": "auto",
  "embeddedLanguageFormatting": "auto"
}
